{
  "buildCommand": "cd clientside && npm install && npm run build",
  "outputDirectory": "clientside/dist",
  "installCommand": "cd clientside && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/user/register",
      "destination": "/api/user/register"
    },
    {
      "source": "/api/user/login", 
      "destination": "/api/user/login"
    },
    {
      "source": "/api/user/get-profile",
      "destination": "/api/user/get-profile"
    },
    {
      "source": "/api/doctor/list",
      "destination": "/api/doctor/list"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
