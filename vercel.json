{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "clientside/dist"
      }
    }
  ],
  "functions": {
    "api/test.js": {
      "runtime": "@vercel/node"
    },
    "api/login.js": {
      "runtime": "@vercel/node"
    },
    "api/register.js": {
      "runtime": "@vercel/node"
    },
    "api/get-profile.js": {
      "runtime": "@vercel/node"
    },
    "api/doctors.js": {
      "runtime": "@vercel/node"
    },
    "api/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/test",
      "dest": "/test-index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
